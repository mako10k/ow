#: src/ow.c:51
#, c-format
msgid "%s\n"
msgstr ""

#: src/ow.c:57
#, c-format
msgid "Usage:\n"
msgstr "使い方:\n"

#: src/ow.c:58
#, c-format
msgid "  %s [options] [--] cmd [arg ...] [redirects]\n"
msgstr ""

#: src/ow.c:59 src/ow.c:71 src/ow.c:78 src/ow.c:93 src/ow.c:100 src/ow.c:105
#, c-format
msgid "\n"
msgstr ""

#: src/ow.c:60
#, c-format
msgid "Options:\n"
msgstr "オプション:\n"

#: src/ow.c:61
#, c-format
msgid "  -i infile     : input file\n"
msgstr "  -i infile     : 入力ファイル\n"

#: src/ow.c:62
#, c-format
msgid "  -o outfile    : output file\n"
msgstr "  -o outfile    : 出力ファイル\n"

#: src/ow.c:63
#, c-format
msgid "  -f inoutfile  : input/output file\n"
msgstr "  -f inoutfile  : 入出力ファイル\n"

#: src/ow.c:64
#, c-format
msgid "  -r renamefile : rename output file\n"
msgstr "  -r renamefile : 出力ファイルのリネーム\n"

#: src/ow.c:65
#, c-format
msgid "  -a            : append mode\n"
msgstr "  -a            : 追記モード\n"

#: src/ow.c:68
#, c-format
msgid "  -p            : punchhole mode (punchhole read data on input file)\n"
msgstr ""
"  -p            : パンチホールモード(入力ファイルで読んだデータを削除)\n"

#: src/ow.c:69
#, c-format
msgid "  -V            : show version\n"
msgstr "  -V            : バージョンの表示\n"

#: src/ow.c:70
#, c-format
msgid "  -h            : show usage\n"
msgstr "  -h            : 使い方の表示\n"

#: src/ow.c:72
#, c-format
msgid "Redirects:\n"
msgstr "リダイレクト:\n"

#: src/ow.c:73
#, c-format
msgid "  < infile      : input file\n"
msgstr "  < infile      : 入力ファイル\n"

#: src/ow.c:74
#, c-format
msgid "  > outfile     : output file\n"
msgstr "  > outfile     : 出力ファイル\n"

#: src/ow.c:75
#, c-format
msgid "  >> outfile    : output file (append mode)\n"
msgstr "  >> outfile    : 出力ファイル(追記モード)\n"

#: src/ow.c:76
#, c-format
msgid "  <> inoutfile  : input/output file\n"
msgstr "  <> inoutfile  : 入出力ファイル\n"

#: src/ow.c:77
#, c-format
msgid "  <>> inoutfile : input/output file (append mode)\n"
msgstr "  <>> inoutfile : 入出力ファイル(追記モード)\n"

#: src/ow.c:79
#, c-format
msgid "  NOTE: You can use same file for input and output.\n"
msgstr "  注意: 入出力で同じファイルを使うことができます。\n"

#: src/ow.c:82
#, c-format
msgid "        It writes to output file only read position to safe read.\n"
msgstr "        書き込みは読み込みが終わった安全な場所にのみ行われます。\n"

#: src/ow.c:85
#, c-format
msgid ""
"        But you shouldn't output widely incresed size data against input\n"
msgstr "        しかし、出力が入力に対して極端に大きな場合は、\n"

#: src/ow.c:86
#, c-format
msgid "        when you use same file for input and output.\n"
msgstr "        同じファイルに入出力すべきではありません。\n"

#: src/ow.c:89
#, c-format
msgid "        It would be stopped program because the all buffer consumed\n"
msgstr "        その場合は読み込み後に書き込みを待っているためのバッファが\n"

#: src/ow.c:92
#, c-format
msgid "        to wait forever writing for read position on the file.\n"
msgstr "        すべて消費されてプログラムが停止することがあります。\n"

#: src/ow.c:94
#, c-format
msgid "  NOTE: < and > must escape or quote on shell.\n"
msgstr "  注意: < と > はシェル上ではエスケープする必要があります。\n"

#: src/ow.c:95
#, c-format
msgid "    example:\n"
msgstr "    例:\n"

#: src/ow.c:98
#, c-format
msgid ""
"      %s -p -r hugefile.txt.gz gzip -c '<hugefile.txt' \\> hugefile.txt\n"
msgstr ""

#: src/ow.c:103
#, c-format
msgid "  NOTE: Using same file for input and output or punchhole option\n"
msgstr ""
"  注意: 同一の入出力ファイルを使うことやパンチホールオプションを使うことは\n"

#: src/ow.c:104
#, c-format
msgid "        may destructive.\n"
msgstr "        ファイルを破壊することがあります。\n"

#: src/ow.c:327
#, c-format
msgid "no file specified for %s\n"
msgstr "%s に対してファイルが指定されていません。\n"

#: src/ow.c:336 src/ow.c:372 src/ow.c:390
#, c-format
msgid "cannot set input file twice or more\n"
msgstr "入力ファイルは2回以上指定できません。\n"

#: src/ow.c:342 src/ow.c:381 src/ow.c:396
#, c-format
msgid "cannot set output file twice or more\n"
msgstr "出力ファイルは2回以上指定できません。\n"

#: src/ow.c:406
#, c-format
msgid "cannot set rename file twice or more\n"
msgstr "リネームファイルは2回以上指定できません。\n"

#: src/ow.c:415
#, c-format
msgid "cannot set append mode twice or more\n"
msgstr "追記モードは2回以上指定できません。\n"

#: src/ow.c:425
#, c-format
msgid "cannot set punchhole mode twice or more\n"
msgstr "パンチホールモードは2回以上指定できません。\n"

#: src/ow.c:493
msgid "cannot set append mode on <stdout>\n"
msgstr "標準出力には追記モードを指定できません。\n"

#: src/ow.c:522
#, c-format
msgid "cannot set punchhole mode for outer redirect\n"
msgstr "外部リダイレクトにはパンチホールモードを指定できません。\n"

#: src/ow.c:576
#, c-format
msgid "cannot rename non regular output\n"
msgstr "通常ファイル以外をリネームできません。\n"

#: src/ow.c:628
#, c-format
msgid "cannot rename to same file\n"
msgstr "同じファイルにリネームできません。\n"

#: src/ow.c:638
#, c-format
msgid "cannot append to non regular file\n"
msgstr "通常ファイル以外を追記モードに指定できません。\n"

#: src/ow.c:748
#, c-format
msgid "buffer exceeded\n"
msgstr "バッファー超過\n"

#: src/ow.c:750
#, c-format
msgid "%s(%ju/%ju) -> %s (buffer = %zu/pipe buffer = %u)\n"
msgstr "%s(%ju/%ju) -> %s (バッファ = %zu/パイプバッファ = %u)\n"

#: src/ow.c:752
msgid "<stdin>"
msgstr "標準入力"

#: src/ow.c:757
#, c-format
msgid "%s(%ju/%ju) <- %s (buffer = %zu/pipe buffer = %u)\n"
msgstr "%s(%ju/%ju) <- %s (バッファ = %zu/パイプバッファ = %u)\n"

#: src/ow.c:759
msgid "<stdout>"
msgstr "標準出力"
